doctype html
html(lang='zh')
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    title= seo && seo.title
    meta(name="keywords" content=seo && seo.keywords)
    meta(name="description" content=seo && seo.description)

    block css
      link(href=assetHashPath("css/vendor.css"), rel="stylesheet")
  body.flex
    include ./_head.pug
    main
      .content
        block content
    include ./_foot.pug
    block js
      script(src=assetHashPath('js/vendor.js'))
